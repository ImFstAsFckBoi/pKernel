{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build: Main",
            "type": "process",
            "command": "make",
            "args": ["-j"],
            "options": {
                "cwd": "${workspaceFolder}",
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "Clean: Main",
            "type": "process",
            "command": "make",
            "args": ["clean", "-j"],
            "options": {
                "cwd": "${workspaceFolder}",
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}
